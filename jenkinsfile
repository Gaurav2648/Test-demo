pipeline{
    
    agent any 
    
    stages{
        stage('build'){
            steps{
                echo "my code is getting build..."
            }
        }
        stage('testing'){
             steps{
                 echo "my code is getting test..."
             }
        }
        stage('code anylaise'){
            steps{
                echo "my code is anyalise..."
            }
        }
        stage('deploy'){
            steps{
                echo "deploy my code..."
            }
        }
    }
}
